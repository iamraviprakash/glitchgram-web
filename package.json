    "postcss": "^8.4.5",
    "prop-types": "^15.7.2",
    "query-string": "^7.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-router-dom": "^6.3.0",
    "sass": "^1.45.0",
    "styletron-engine-atomic": "^1.4.8",
